{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=PruebaBNA.db"
  },
  "CacheSettings": {
    "ExpirationInMinutes": 60,
    "BcraExpirationSeconds": 3600
  },
  "ExternalServices": {
    "BcraApiUrl": "https://api.bcra.gob.ar/centraldedeudores/v1.0/"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}